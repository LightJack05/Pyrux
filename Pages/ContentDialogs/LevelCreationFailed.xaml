<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="Pyrux.Pages.ContentDialogs.LevelCreationFailed"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Pyrux.Pages.ContentDialogs"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="Transparent">

    <ScrollViewer Background="Transparent">
        <StackPanel Background="Transparent">
            <TextBlock FontSize="15" TextWrapping="WrapWholeWords">Maybe a level with this name already exists, the name contains invalid characters for a filename, or you did not enter a name.</TextBlock>
        </StackPanel>
    </ScrollViewer>
</Page>
